{
  "name": "bitset",
  "main": "bitset.js",
  "module": "bitset.esm.js",
  "types": "bitset.d.ts",
  "scripts": {
    "dev": "cd app && npm run dev",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "generate-benchmark:set": "ts-node -T benchmarks/set/generate",
    "benchmark:set": "ts-node -T benchmarks/set/run",
    "generate-benchmark:setRange": "ts-node -T benchmarks/setRange/generate",
    "benchmark:setRange": "ts-node -T benchmarks/setRange/run",
    "generate-benchmark:and": "ts-node -T benchmarks/and/generate",
    "benchmark:and": "ts-node -T benchmarks/and/run",
    "generate-benchmark:or": "ts-node -T benchmarks/or/generate",
    "benchmark:or": "ts-node -T benchmarks/or/run",
    "generate-benchmark:andNot": "ts-node -T benchmarks/andNot/generate",
    "benchmark:andNot": "ts-node -T benchmarks/andNot/run"
  },
  "devDependencies": {
    "bitset": "^5.1.1",
    "ts-node": "^10.9.1",
    "vitest": "^0.34.6"
  },
